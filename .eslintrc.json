{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "quote": "double",
        "require-jsdoc":"off"
    }
}
